{
  "name": "whataboutme",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:prod": "ng serve --proxy-config=proxy.conf.json",
    "start:dev": "ng serve -c=dev --port=4201 --proxy-config=proxy/dev-proxy.conf.json",
    "start:qa": "ng serve -c=qa --port=4202 --proxy-config=proxy/qa-proxy.conf.json",
    "build": "ng build --prod",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../whataboutme/src/main/resources/static/ && mkdirp ../whataboutme/src/main/resources/static/",
    "deploy": "copyfiles -f dist/whataboutme/** ../whataboutme/src/main/resources/static/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "angular-webstorage-service": "^1.0.2",
    "bootstrap": "^4.3.1",
    "copyfiles": "^2.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "ngx-webstorage-service": "^4.1.0",
    "popper.js": "^1.15.0",
    "rimraf": "^3.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.2",
    "@angular/cli": "~9.1.15",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/language-service": "~12.0.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~4.2.4"
  }
}
