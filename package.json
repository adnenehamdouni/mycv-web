{
  "name": "mycv-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:prod": "ng serve --proxy-config=proxy.conf.json",
    "start:dev": "ng serve -c=dev --port=4201 --proxy-config=proxy/dev-proxy.conf.json",
    "start:qa": "ng serve -c=qa --port=4202 --proxy-config=proxy/qa-proxy.conf.json",
    "build": "ng build --configuration production",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../mycv/src/main/resources/static/ && mkdirp ../mycv/src/main/resources/static/",
    "deploy": "copyfiles -f dist/mycv/** ../mycv/src/main/resources/static/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/platform-browser": "^12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@ngrx/effects": "^12.0.0",
    "@ngrx/store": "^12.0.0",
    "bootstrap": "^4.3.1",
    "copyfiles": "^2.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "ngx-owl-carousel-o": "~5.0.0",
    "ngx-webstorage-service": "^4.1.0",
    "popper.js": "^1.15.0",
    "rimraf": "^3.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.2",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/language-service": "~12.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ajv": "^6.12.6",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
